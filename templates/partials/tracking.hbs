{{#*inline "satellite"}}
  <script>
    _satellite.pageBottom();
  </script>
{{/inline}}
{{#if articleBody.tracking-gtm-gpo}}
  <script>(function(w,d,s,l,i){w[l]=w[l]||[];w[l].push({'gtm.start':
    new Date().getTime(),event:'gtm.js'});var f=d.getElementsByTagName(s)[0],
    j=d.createElement(s),dl=l!='dataLayer'?'&l='+l:'';j.async=true;j.src=
    'https://www.googletagmanager.com/gtm.js?id='+i+dl;f.parentNode.insertBefore(j,f);
    })(window,document,'script','dataLayer','{{{articleBody.tracking-gtm-gpo}}}');
  </script>
{{/if}}
{{#if articleBody.tracking-gtm-client}}
  {{#potentialAction 'renderHTML'}}
    {{#compare assemblyVersion '!=' 'development'}}
      <script>(function(w,d,s,l,i){w[l]=w[l]||[];w[l].push({'gtm.start':
        new Date().getTime(),event:'gtm.js'});var f=d.getElementsByTagName(s)[0],
        j=d.createElement(s),dl=l!='dataLayer'?'&l='+l:'';j.async=true;j.src=
        'https://www.googletagmanager.com/gtm.js?id='+i+dl;f.parentNode.insertBefore(j,f);
        })(window,document,'script','dataLayer','{{{@root.articleBody.tracking-gtm-client}}}');
      </script>
    {{/compare}}
  {{/potentialAction}}
{{/if}}
{{#if articleBody.tracking-dtm-production}}
  {{#potentialAction 'renderHTML'}}
    {{#compare assemblyVersion '==' 'production'}}
      {{#contains @root.articleBody.tracking-dtm-production "satellite"}}
        {{>satellite}}
      {{/contains}}
    {{else}}
      {{#compare assemblyVersion '==' 'staging'}}
        {{#contains @root.articleBody.tracking-dtm-staging "satellite"}}
          {{>satellite}}
        {{/contains}}
      {{/compare}}
    {{/compare}}
  {{/potentialAction}}
{{/if}}